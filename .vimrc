
syntax enable

set t_Co=256
colorscheme default

set number relativenumber
set nu rnu
set ruler
set ttyfast
set tabstop=2
set shiftwidth=4
set smarttab
set expandtab
set autoindent
set cursorline
set showmatch
set enc=utf-8
set nobackup
set nowritebackup
set noswapfile
set backspace=indent,eol,start
set scrolloff=10
set clipboard=unnamed
set exrc
set secure

tab sball
set switchbuf=useopen
set laststatus=2
nmap <F9> :bprev<CR>
nmap <F10> :bnext<CR>

set incsearch
set hlsearch

